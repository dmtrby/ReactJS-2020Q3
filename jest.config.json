{
  "testRegex": "((\\.|/*.)(test))\\.js?$",
  "testEnvironment": "jsdom",
  "transformIgnorePatterns": ["node_modules"],
  "moduleNameMapper": {
    "\\.(css|scss)$": "<rootDir>/mocks/styleMock.js"
  },
  "collectCoverage": true,
  "collectCoverageFrom": ["src/**/*.{js,jsx}"],
  "reporters": ["default", "jest-coverage-guard"],
  "verbose": true
}